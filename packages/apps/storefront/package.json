{
  "name": "storefront",
  "version": "0.1.0",
  "private": true,
  "description": "Based on v3.6.0 of @oracle-cx-commerce/blank-store",
  "dependencies": {
    "@oracle-cx-commerce/actions": "3.6.0",
    "@oracle-cx-commerce/endpoints": "3.6.0",
    "@oracle-cx-commerce/react-app": "3.6.0",
    "@oracle-cx-commerce/react-components": "3.6.0",
    "@oracle-cx-commerce/react-widgets": "3.6.0",
    "@oracle-cx-commerce/styles": "3.6.0",
    "@oracle-cx-commerce/subscribers": "3.6.0",
    "@oracle-cx-commerce/commerce-utils": "3.6.0"
  },
  "devDependencies": {
    "@oracle-cx-commerce/rollup-config": "3.6.0"
  },
  "occ": {
    "namespace": "occ.react",
    "plugins": {
      "actions": "src/plugins/actions",
      "components": "src/plugins/components",
      "endpoints": "src/plugins/endpoints",
      "subscribers": "src/plugins/subscribers"
    },
    "locales": [
      "en",
      "de"
    ],
    "aliases": {
      "@oracle-cx-commerce/styles/common.css": "storefront/src/core/ui/styles/common.css"
    }
  }
}